{
  "name": "vscode-specificity",
  "displayName": "Specificity",
  "description": "CSS Specificity extension for VS Code",
  "version": "1.0.0",
  "main": "dist/extension.js",
  "publisher": "kerihenare",
  "author": "Keri Henare <keri@henare.co.nz>",
  "license": "MIT",
  "homepage": "https://github.com/kerihenare/specificity",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kerihenare/specificity.git"
  },
  "bugs": {
    "url": "https://github.com/kerihenare/specificity/issues"
  },
  "keywords": [
    "css",
    "specificity"
  ],
  "categories": [
    "Other"
  ],
  "scripts": {
    "build": "NODE_ENV=production babel src --out-dir build --extensions '.ts'",
    "test": "jest --passWithNoTests",
    "test:coverage": "jest --coverage --passWithNoTests",
    "test:lint": "eslint --ext .ts src",
    "test:type-check": "tsc --noEmit",
    "vscode:prepublish": "npm run build"
  },
  "engines": {
    "vscode": "^1.32.0"
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/cli": "^7.10.4",
    "@babel/core": "^7.10.4",
    "@babel/node": "^7.10.4",
    "@babel/preset-env": "^7.10.4",
    "@babel/preset-typescript": "^7.10.4",
    "@types/jest": "^26.0.3",
    "@types/node": "^14.0.14",
    "@types/vscode": "^1.46.0",
    "@typescript-eslint/eslint-plugin": "^3.5.0",
    "@typescript-eslint/parser": "^3.5.0",
    "eslint": "^7.3.1",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jest": "^23.17.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-security": "^1.4.0",
    "eslint-plugin-standard": "^4.0.1",
    "husky": "^4.2.5",
    "jest": "^26.1.0",
    "lint-staged": "^10.2.11",
    "prettier": "^2.0.5",
    "ts-jest": "^26.1.1",
    "typescript": "^3.9.5"
  },
  "activationEvents": [],
  "contributes": {},
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix"
    ]
  }
}
